<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>CheckoutReg</servlet-name>
        <servlet-class>adminpackage.CheckoutServletReg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckoutRegservlet</servlet-name>
        <servlet-class>adminpackage.CheckoutRegservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Recipt</servlet-name>
        <servlet-class>adminpackage.Recipt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cart</servlet-name>
        <servlet-class>adminpackage.cart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteCart</servlet-name>
        <servlet-class>adminpackage.deleteCart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteCartItem</servlet-name>
        <servlet-class>adminpackage.DeleteCartItem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DelCartUnReg</servlet-name>
        <servlet-class>adminpackage.DelCartUnReg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DelCartItemUnreg</servlet-name>
        <servlet-class>adminpackage.DelCartItemUnreg</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckoutReg</servlet-name>
        <url-pattern>/CheckoutReg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckoutRegservlet</servlet-name>
        <url-pattern>/CheckoutRegservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Recipt</servlet-name>
        <url-pattern>/Recipt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cart</servlet-name>
        <url-pattern>/cart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteCart</servlet-name>
        <url-pattern>/deleteCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteCartItem</servlet-name>
        <url-pattern>/DeleteCartItem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DelCartUnReg</servlet-name>
        <url-pattern>/DelCartUnReg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DelCartItemUnreg</servlet-name>
        <url-pattern>/DelCartItemUnreg</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
